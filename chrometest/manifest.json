  "description": "AUDIO-PLUGIN",
  "browser_action": {
    "default_icon": "/src/images/icon.png",
    "default_title": "音频数据抓取123",
    "default_popup": "index.html"   
  },
  "icons": {
    "48": "/src/images/icon.png"
  },
  "background": {
    "page": "background.html"
  },
